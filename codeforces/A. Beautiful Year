#include <iostream>
#include <set>
#include <string>

using namespace std;

bool distinct(int num) {
	set<char> table;
	string numStr = to_string(num);
	for (auto n : numStr) {
		table.insert(n);
	}

	return numStr.length() == table.size();
}

int main()
{
	int n;
	cin >> n;

	int i = n+1;
	while (true) {
		if (distinct(i)) {
			cout << i << endl;
			return 0;
		}
		i++;
	}
}
