#include <iostream>
#include <string>

using namespace std;

int main()
{
    int n;
    cin >> n;
    
    string s;
    cin >> s;

    int count = 0;
    if (s != "0") {
        for (int i = 0; i < s.length(); i++) {
            if (s[i] == '0') count++;
        }
        
        cout << "1";
        for (int i = 0; i < count; i++) {
            cout << "0";
        }
        
        cout << endl;
    } else {
        cout << "0" << endl;
    }
}
