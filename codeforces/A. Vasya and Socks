#include <iostream>

using namespace std;

int main()
{
    int n, m;
    cin >> n >> m;
    
    int i = 1;
    while (true) {
        n -= 1;
        if (i%m == 0) n += 1;
        
        if (n == 0) {
            cout << i << endl;
            break;
        }
        
        i++;
        
        
    }
}
