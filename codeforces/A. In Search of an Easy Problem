#include <iostream>
#include <cstdlib>
#include <vector>
#include <string>

using namespace std;

int main() {
	ios_base::sync_with_stdio(false);
	string length;
	string word;
	
	getline(cin, length);
	while (cin >> word) {
		if (word == "1") {
			cout << "HARD" << endl;
			return 0;
		}
	}
	cout << "EASY" << endl;
}
