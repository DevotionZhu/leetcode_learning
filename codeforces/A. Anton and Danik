#include <iostream>
#include <cstdlib>
#include <string>

using namespace std;

int main()
{
	string inputNum;
	string inputResult;
	cin >> inputNum;

	int total = stoi(inputNum.c_str());
	int i = 0;	
	while (cin >> inputResult) {
		if (inputResult == "A") {
			i++;
		}
	}
	
	if (total == i*2) {
		cout << "Friendship" << endl;
	} else if (total > i*2) {
		cout << "Danik" << endl;
	} else {
		cout << "Anton" << endl;
	}
	return 0;	
}
