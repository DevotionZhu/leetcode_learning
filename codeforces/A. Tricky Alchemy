#include <iostream>

using namespace std;

int main()
{
    long long a, b;
    cin >> a >> b;
    
    long long x, y ,z;
    cin >> x >> y >> z;
    
    long long res1 = 2*x + y - a ;
    long long res2 = y + 3*z - b;
    
    if (res1 < 0) res1 = 0;
    if (res2 < 0) res2 = 0;
    
    cout << (res1 + res2)  << endl;
}
