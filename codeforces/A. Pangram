#include <iostream>
#include <string>
#include <set>

using namespace std;

int main()
{
	int n;
	string letters;

	cin >> n >> letters;

	set<char> table;
	for (int i = 0; i < n; i++) {
		char t = tolower(letters[i]);
		table.insert(t);
	}

	if (table.size() == 26) {
		cout << "YES" << endl;
	} else {
		cout << "NO" << endl;
	}
}
