#include <iostream>
#include <cstdlib>
#include <string>


using namespace std;

int main() {
	ios_base::sync_with_stdio(false);
	string inputNum;
	string inputJuice;
	cin >> inputNum;
	
	double sum = 0;
	long total = atoi(inputNum.c_str());
	while (cin >> inputJuice) {
		sum += atoi(inputJuice.c_str());
	}
	
	cout << sum / total << endl;
}
